<body class="content">
  <div class="center">
  <h1 class="inline">See all decks</h1>
</div>
<br><br>
  <ul id="deck-list">
    <% @decks.each do |deck| %>
      <li>
        <%= link_to deck.name, deck_path(deck) %>
        <br>
        <%= deck.description %>
      </li>
    <% end %>
    <li>
      <%= link_to(content_tag(:i,nil,class: "fa fa-plus"), new_deck_path) %>
    </li>
  </ul>
</body>
